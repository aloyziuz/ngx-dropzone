{
  "name": "@aloysius-software-factory/ngx-dropzone",
  "private": false,
  "scripts": {
    "start:app": "ng serve app",
    "start:cdk": "ng build --watch --project=cdk",
    "build:cdk": "ng build cdk --configuration production",
    "test:cdk": "ng test cdk --no-watch --no-progress --browsers=ChromeHeadless",
    "lint:cdk": "ng lint cdk",
    "start:material": "ng build --watch --project=material",
    "build:material": "ng build material --configuration production",
    "test:material": "ng test material --no-watch --no-progress --browsers=ChromeHeadless",
    "lint:material": "ng lint material", 
    "publish:cdk": "npm run build:cdk && cd dist/cdk && npm publish --access public",
    "publish:material": "npm run build:material && cd dist/material && npm publish --access public", 
    "publish:all": "npm run publish:cdk && npm run publish:material"
  },
  "dependencies": {
    "@angular/animations": "^16.1.4",
    "@angular/cdk": "~16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/material": "~16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular-eslint/builder": ">=17.2.1",
    "@angular-eslint/eslint-plugin": ">=17.2.1",
    "@angular-eslint/eslint-plugin-template": ">=17.2.1",
    "@angular-eslint/schematics": ">=17.2.1",
    "@angular-eslint/template-parser": ">=17.2.1",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.2",
    "typescript": "~5.3.3"
  }
}